<VBox alignment="CENTER" prefHeight="405.0" prefWidth="578.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.proyecto2doparcial.SecondaryController">
